@model IEnumerable<MVC_Garage_2._0.Models.ViewModels.VehicleStats>

@{
    ViewBag.Title = "Garage Statistics";
}

<h2>Garage Statistics</h2>

<p>
   
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegNo)
        </th>       
        <th>
            @Html.DisplayNameFor(model => model.ParkedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalParkedCost)
        </th>
        <th>
           
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalParkedCost)
        </td>
        
        <td>
            
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.Id })
           
        </td>
    </tr>
}

</table>
<hr />
<h3>Parked Vehicles Summary</h3>
@foreach(var veh in ViewBag.Text)
{
    <li style="background-color:antiquewhite">
        @veh
    </li>
}
