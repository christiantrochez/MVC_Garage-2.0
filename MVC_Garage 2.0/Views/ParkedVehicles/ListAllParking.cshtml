@model IEnumerable<MVC_Garage_2._0.Models.ViewModels.ListAllParkingViewModel>
@{
    ViewBag.Title = "List All PArking Spots";
}

<h2>List All Parking Spots</h2>


      


<table>

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParkingSpotNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParkingSpotStatus)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingSpotNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingSpotStatus)
            </td>
        </tr>
    }

</table>

